datos <- data.frame(
rigidez = c(108, 115, 112,   # Conc 5
122, 118, 116,   # Conc 10
132, 114, 130,   # Conc 15
138, 125, 145,   # Conc 20
142, 148, 140,   # Conc 25
118, 116, 120,   # Conc 30
132, 128, 124),  # Conc 35
